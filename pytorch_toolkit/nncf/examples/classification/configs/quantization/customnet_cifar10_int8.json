{
    "model": "customnet",
    "num_classes": 10,
    "pretrained": false,
    "batch_size": 256,
    "input_sample_size": [2, 3, 32, 32],
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.0001,
        "schedule_type": "multistep",
        "steps": [20, 45]
    },
    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "num_init_steps": 1
        },
        "activations": {
            "mode": "asymmetric"
        },
        "weights": {
            "mode": "asymmetric"
        }
    },
    "epochs": 100,
    "dataset": "CIFAR10"
}
